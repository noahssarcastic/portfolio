{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "This app will serve as an end-to-end software development portfolio.",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development ts-node ./server/index.ts",
    "start:prod": "NODE_ENV=production ts-node --transpile-only ./server/index.ts",
    "watch": "NODE_ENV=development nodemon ./server/index.ts",
    "build": "NODE_ENV=development webpack --config webpack.dev.ts",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.ts",
    "lint": "eslint . --ext '.js, .jsx, .ts, .tsx'",
    "fmt": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "clean": "rimraf node_modules package-lock.json dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahssarcastic/portfolio.git"
  },
  "keywords": [
    "node",
    "portfolio"
  ],
  "author": "Noah Iniguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noahssarcastic/portfolio/issues"
  },
  "homepage": "https://github.com/noahssarcastic/portfolio#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-middleware": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.15",
    "pinst": "^2.0.0",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "packageManager": "yarn@3.1.1"
}
